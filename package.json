{
  "description": "The official website for scsys, a company specializing in decentralized systems and applications.",
  "homepage": "https://scsys.io",
  "license": "Apache-2.0",
  "private": true,
  "name": "@scsys/scsys-io",
  "version": "0.0.6",
  "author": {
    "name": "scsys",
    "email": "support@scsys.io",
    "url": "https://github.com/scattered-systems"
  },
  "bugs": {
    "url": "https://github.com/scattered-systems/scsys-io/issues"
  },
  "engine": {
    "node": ">=22",
    "npm": ">10"
  },
  "keywords": [
    "website",
    "scsys"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scattered-systems/scsys-io"
  },
  "scripts": {
    "build": "npm run cf:build",
    "deploy": "npm run cf:deploy",
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "lint": "next lint",
    "start": "next start",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "supabase:init": "npx supabase init",
    "supabase:push": "npx supabase db push",
    "supabase:pull": "npx supabase db pull",
    "supabase:reset": "npx supabase db reset",
    "supabase:link": "npx supabase link",
    "supabase:types": "npx supabase gen types typescript --project-id gilqgzjkzkmhbbcqidqb --schema public > ./src/types/database.types/database.public.types.ts",
    "shadcn:add": "npx shadcn@latest add",
    "shadcn:registry": "npx shadcn@latest registry:build",
    "shadcn:init": "npx shadcn@latest init",
    "vercel:pull-env": "npx vercel@latest env pull",
    "cf:build": "opennextjs-cloudflare build",
    "cf:deploy": "opennextjs-cloudflare build && npm run wrangler deploy",
    "cf:preview": "opennextjs-cloudflare preview",
    "wrangler": "npx wrangler@latest",
    "wrangler:typegen": "npx wrangler@latest types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
